<div class="qor-field">
  <div class="mdl-textfield mdl-textfield--full-width mdl-js-textfield">
    <label class="qor-field__label mdl-textfield__label" for="{{.InputId}}">
      {{meta_label .Meta}}
    </label>

    <div class="qor-field__show">{{.Value.OrderSN}}</div>

    <div class="qor-field__edit">
      <input class="mdl-textfield__input" type="text" id="{{.InputId}}" name="{{.InputName}}" value="{{.Value.OrderSN}}" {{if (not (has_change_permission .Meta)) }}disabled{{end}}>
    </div>
  </div>
</div>
